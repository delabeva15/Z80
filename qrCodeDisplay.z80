    ld de,plotSScreen
    ld hl,Qr
    ld b,100
    ld c,4
    
QrLoop:
	ld a,(hl)
    ld (de),a
    inc hl
    inc de
    dec c
    dec b
    ld a,c
    cp 0
    jr nz,QrLoop
    ld a,8
    call nextCollumn
    ld c,4
    ld a,b
    cp 0
    jr nz,QrLoop
    bcall(_GrBufCpy)
    ret 
    
nextCollumn:
	inc de
    dec a
    cp 0
    jr nz,nextCollumn
    ret z
    
